{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ic10",
      "camelcase": "IC10",
      "title": "IC10",
      "scope": "source.ic10",
      "file-types": [
        "ic10"
      ],
      "injection-regex": "^ic10$",
      "class-name": "TreeSitterIc10Typed"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Typed IC10 grammar for Stationeers. IC10 is the game's virtual embedded language used for automation and control systems.",
    "authors": [
      {
        "name": "Joel Hillare",
        "email": "joel@403forbidden.ca"
      }
    ],
    "links": {
      "repository": "https://github.com/jhillacre/tree-sitter-ic10-type"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}